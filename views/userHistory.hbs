<div class="row">
    <div class="col-md-12">
        <h2>{{session.user.userName}} ( {{session.user.email}} ) History</h2>
        <hr>
        <div class="table-responsive">
            <table class="table">
                <thead>
                    <th>Login Data/Time</th>
                    <th>Client Information</th>
                </thead>
                <tbody>
                    {{#each session.user.loginHistory}}
                        <tr>
                            <td>{{dateTime}}</td>
                            <td>{{userAgent}}</td>
                        </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>